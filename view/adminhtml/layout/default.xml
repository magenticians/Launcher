<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <script src="Magenticians_Launcher::js/launcher.js" />
        <css src="Magenticians_Launcher::css/launcher.css" />
    </head>

    <body>
        <referenceContainer name="header.inner.right">
            <block class="Magento\Framework\View\Element\Template" before="notification.messages" name="launcher_toolbar_entry" template="Magenticians_Launcher::toolbar_entry.phtml" />
        </referenceContainer>

        <referenceContainer name="before.body.end">
            <block class="Magenticians\Launcher\Block\LauncherItems" name="launcher_items" />
        </referenceContainer>
    </body>
</page>